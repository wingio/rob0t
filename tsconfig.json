{
    "compilerOptions": {
      "outDir": "dist",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "lib": ["esnext", "esnext.array", "esnext.asynciterable", "esnext.symbol", "DOM"],
      "module": "commonjs",
      "moduleResolution": "node",
      "strict": true,
      "noImplicitAny": false,
      "target": "ESNEXT",
      "baseUrl": ".",
      "paths": {
          "~/*": ["src/*"],
      },
      "skipLibCheck": true
    },
    "include": ["src/**/*"],
    
  }